      </option>
      <% }); %>
  </select>
  <label for="model">Model</label>
  <select name="model_dropdown" id="model">
    <option></option>
    <% models.forEach(m => { %>
      <option>
        <%= m.Model %>
      </option>
      <% }); %>
  </select>
  <label for="material">Materia≈Ç</label>
  <select name="material_dropdown" id="material">
    <option></option>
    <% materials.forEach(m => { %>
      <option>
        <%= m.Material %>
      </option>
      <% }); %>
  </select>
  <label for="color">Kolor</label>
  <select name="color_dropdown" id="color">
    <option></option>
    <% colors.forEach(c => { %>
      <option>
        <%= c.Kolor %>
      </option>
      <% }); %>
  </select>
